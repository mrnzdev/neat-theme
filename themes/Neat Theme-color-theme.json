{
	"name": "Neat Theme",
	"colors": {
		"foreground": "#ffffff",
		"focusBorder": "#000000",
		"progressBar.background": "#ffffff",
		"selection.background": "#000000",
		"scrollbar.shadow": "#000000",
		"activityBar.foreground": "#ffffff",
		"activityBar.background": "#000000",
		"activityBar.inactiveForeground": "#ffffff7f",
		"activityBarBadge.foreground": "#ffffff",
		"activityBarBadge.background": "#000000",
		"activityBar.border": "#000000",
		"activityBar.activeBackground": "#000000",
		"sideBar.background": "#000000",
		"sideBar.foreground": "#ffffff",
		"sideBarSectionHeader.background": "#000000",
		"sideBarSectionHeader.border": "#000000",
		"sideBarSectionHeader.foreground": "#ffffff",
		"sideBar.border": "#000000",
		"list.inactiveSelectionBackground": "#000000",
		"list.inactiveSelectionForeground": "#ffffff7f",
		"list.hoverBackground": "#000000",
		"list.hoverForeground": "#ffffff",
		"list.activeSelectionBackground": "#000000",
		"list.activeSelectionForeground": "#ffffff",
		"tree.indentGuidesStroke": "#ffffff",
		"list.dropBackground": "#000000",
		"list.highlightForeground": "#ffffff",
		"list.focusBackground": "#000000",
		"list.focusForeground": "#ffffff7f",
		"listFilterWidget.background": "#000000",
		"listFilterWidget.outline": "#ffffff7f",
		"listFilterWidget.noMatchesOutline": "#ffffff7f",
		"statusBar.foreground": "#ffffff7f",
		"statusBar.background": "#000000",
		"statusBarItem.hoverForeground": "#ffffff",
		"statusBar.border": "#000000",
		"statusBarItem.remoteBackground": "#000000",
		"statusBarItem.remoteForeground": "#ffffff7f",
		"statusBarItem.remoteHoverForeground": "#ffffff"
	},
	"tokenColors": [
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "meta.diff.header",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"constant.other.color.rgb-value",
				"constant.other.rgb-value",
				"support.constant.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": "constant.regexp",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": "entity.name.tag.css",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": [
				"source.css entity.other.attribute-name",
				"source.css.less entity.other.attribute-name.id",
				"source.scss entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#ff0000"
			}
		},
		{
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "markup.heading",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": "markup.changed",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": "meta.selector",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "brackets of XML/HTML tags",
			"scope": ["punctuation.definition.tag"],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": "meta.preprocessor",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": "meta.preprocessor.string",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": "meta.preprocessor.numeric",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": "meta.structure.dictionary.key.python",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": "storage.modifier",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": "string.tag",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": "string.value",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": "string.regexp",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "JavaScript string interpolation ${}",
			"scope": [
				"punctuation.definition.template-expression.begin.js",
				"punctuation.definition.template-expression.begin.ts",
				"punctuation.definition.template-expression.end.ts",
				"punctuation.definition.template-expression.end.js"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": [
				"support.type.vendored.property-name",
				"support.type.property-name",
				"variable.css",
				"variable.scss",
				"variable.other.less"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": "keyword.control",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": [
				"keyword.operator.new",
				"keyword.operator.expression",
				"keyword.operator.cast",
				"keyword.operator.sizeof",
				"keyword.operator.logical.python"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": "keyword.other.unit",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": "support.function.git-rebase",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": "constant.sha.git-rebase",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "coloring of the Java import and package identifiers",
			"scope": [
				"storage.modifier.import.java",
				"variable.language.wildcard.java",
				"storage.modifier.package.java"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "coloring of the TS this",
			"scope": "variable.language.this",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Function declarations",
			"scope": [
				"entity.name.function",
				"support.function",
				"support.constant.handlebars"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Types declaration and references",
			"scope": [
				"meta.return-type",
				"support.class",
				"support.type",
				"entity.name.type",
				"entity.name.class",
				"source.cs storage.type"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Types declaration and references, TS grammar specific",
			"scope": [
				"meta.type.cast.expr",
				"meta.type.new.expr",
				"support.constant.math",
				"support.constant.dom",
				"support.constant.json",
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Control flow keywords",
			"scope": "keyword.control",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Variable and parameter name",
			"scope": [
				"variable",
				"meta.definition.variable.name",
				"support.variable"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Object keys, TS grammar specific",
			"scope": [
				"meta.object-literal.key",
				"meta.object-literal.key entity.name.function"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "CSS property value",
			"scope": [
				"support.constant.property-value",
				"support.constant.font-name",
				"support.constant.media-type",
				"support.constant.media",
				"constant.other.color.rgb-value",
				"constant.other.rgb-value",
				"support.constant.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		}
	]
}
